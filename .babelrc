{
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    ["@babel/plugin-proposal-object-rest-spread", { "useBuiltIns": true }]
  ],
  "env": {
    "test": {
      "plugins": [
        "istanbul"
      ],
      "presets": [
        [
          "@babel/preset-env", {
          "debug": true,
          "modules": false,
          "exclude": [
            "web.dom.iterable"
          ],
          "targets": {
            "chrome": 62,
            "firefox": 57,
            "edge": 15,
            "safari": 11
          }
        }],
        "@babel/preset-react"
      ]
    },
    "es": {
      "presets": [
        [
          "@babel/preset-env", {
          "debug": true,
          "modules": false,
          "useBuiltIns": "usage",
          "exclude": [
            "web.dom.iterable"
          ],
          "targets": {
            "chrome": 62,
            "firefox": 57,
            "edge": 15,
            "safari": 11
          }
        }],
        "@babel/preset-react"
      ]
    },
    "cjs": {
      "presets": [
        [
          "@babel/preset-env", {
          "debug": true,
          "modules": "commonjs",
          "useBuiltIns": "usage",
          "exclude": [
            "web.dom.iterable"
          ],
          "targets": {
            "chrome": 62,
            "firefox": 57,
            "edge": 15,
            "safari": 11
          }
        }],
        "@babel/preset-react"
      ]
    }
  }
}
